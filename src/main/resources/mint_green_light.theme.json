{
  "name": "Mint Green Light",
  "dark": false,
  "author": "Ingo Steinke (openmindculture)",
  "editorScheme": "/mint_green_light.xml",
  "colors": {
    "white": "#ffffff",
    "greenishLight": "#f3f4d4",
    "thunarMint": "#5aaa9a",
    "greenishYellow": "#b6d600",
    "lightConsole": "#caf4b7",
    "pistachioYellow": "#f7fc99",
    "fadedFooter": "#a7d1b6",
    "neonGrass": "#79c310",
    "mellowFlow": "#95a7b4",
    "greenEucalyptus": "#728660",
    "transformativeTeal": "#2f6364",
    "neonPastel": "#aff1bb",
    "neonMint": "#6ae9b1"
  },
  "ui": {
    "*": {
      "background": "lightConsole",
      "selectedBackground": "pistachioYellow",
      "hoverBackground": "greenishYellow",
      "foreground": "mellowFlow",
      "disabledText": "thunarMint",
      "selectionBackground": "neonGrass",
      "selectionForeground": "greenishLight",
      "underlineColor": "thunarMint",
      "errorFocusColor": "neonGrass"
    },
    "Editor.background": "lightConsole",
    "Button.default.foreground": "greenishLight",
    "Borders": {
      "color": "neonMint",
      "ContrastBorderColor": "neonPastel"
    },
    "Panel.foreground": "transformativeTeal",
    "Panel.foreground.hover": "greenEucalyptus",
    "Panel.hoverForeground": "greenEucalyptus",
    "ScrollPane.background": "lightConsole",
    "ScrollPane.foreground": "greenEucalyptus",
    "TextArea.foreground": "greenEucalyptus",
    "TextField.background": "pistachioYellow",
    "TextField.foreground": "greenEucalyptus",
    "TextField.inactiveForeground": "greenEucalyptus",
    "Viewport.foreground": "greenEucalyptus"
  }
}
